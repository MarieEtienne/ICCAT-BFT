@article{Schnute+95,
    abstract = {{Catch-age analysis has become a standard tool for modern stock assessment, where it is recognized that population estimates are linked to estimates of the natural mortality rate M. We use state space models and simulation-estimation cycles to evaluate the performance of catch-age analysis. In particular, we design a simulation model with realistic measurement and process error. We then examine how effectively unknown parameters and population sizes can be obtained from an estimation model simplified to conform with common assumptions. We describe circumstances in which, simultaneously with all other parameters, M is estimable from traditional catch-age data. Increased fishing contrast improves estimates of some model parameters, but not M. The variance ratio rho relating process error in recruitments with measurement error in the abundance index must be specified in the estimation. We consider both true and ad hoc choices for rho. In general, estimation models perform better when the true rho is specified and measurement error is low, so that recruitment error is the dominant source of model variability. Thus, well-designed data collection programs are critical to the correct interpretation of actual fishery data.}},
    author = {Schnute, J. T. and Richards, L. J.},
    citeulike-article-id = {6247056},
    keywords = {file-import-2009-11-30},
    number = {10},
    pages = {2063--2077},
    posted-at = {2009-11-30 18:34:55},
    priority = {2},
    title = {{The influence of error on population estimates from catch-age models}},
    volume = {52},
    year = {1995}
}

